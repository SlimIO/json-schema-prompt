{
    "name": "@slimio/json-schema-prompt",
    "version": "1.0.0",
    "description": "Tool to verify and create some json schema",
    "main": "./index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "test": "cross-env psp && ava --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/json-schema-prompt.git"
    },
    "keywords": [
        "jsp",
        "tool",
        "slimio",
        "json-schema-prompt",
        "json"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/json-schema-prompt/issues"
    },
    "homepage": "https://github.com/SlimIO/json-schema-prompt#readme",
    "dependencies": {
        "kleur": "^3.0.3",
        "lodash.get": "^4.4.2",
        "lodash.set": "^4.3.2",
        "qoa": "^0.2.0",
        "yn": "^3.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.3",
        "@slimio/psp": "^0.8.0",
        "ava": "^2.3.0",
        "cross-env": "^5.2.0",
        "dotenv": "^8.1.0",
        "eslint": "^6.2.2",
        "husky": "^3.0.4",
        "jsdoc": "^3.6.3",
        "nyc": "^14.1.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
