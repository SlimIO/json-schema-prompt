{
    "name": "@slimio/json-schema-prompt",
    "version": "1.0.1",
    "description": "Tool to verify and create some json schema",
    "main": "./index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "test": "cross-env psp && nyc --reporter=lcov ava --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/json-schema-prompt.git"
    },
    "files": [
        "index.d.ts"
    ],
    "keywords": [
        "jsp",
        "tool",
        "slimio",
        "json-schema-prompt",
        "json"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/json-schema-prompt/issues"
    },
    "homepage": "https://github.com/SlimIO/json-schema-prompt#readme",
    "dependencies": {
        "kleur": "^4.0.3",
        "lodash.set": "^4.3.2",
        "qoa": "^0.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/psp": "^0.11.1",
        "ava": "^3.8.2",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "jsdoc": "^3.6.4",
        "nyc": "^15.0.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
